    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Medication_adherence</IRI>
        <Literal datatypeIRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral">418633004 | Medication adherence |</Literal>
