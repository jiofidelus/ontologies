        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">62014003</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Medication_adverse_effects</IRI>
