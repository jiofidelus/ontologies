    <!-- http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Direct -->

    <owl:Class rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Direct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Direct/Indirect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#It_is_Kind_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Biological"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Biological_markers"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Blood"/>
