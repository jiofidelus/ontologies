        <obo:IAO_0000119>Dictionary of Epidemiology, Miquel Porta (Ed.)</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/epo.owl"/>
        <rdfs:label>cross-product ratio</rdfs:label>
        <rdfs:seeAlso>http://purl.obolibrary.org/obo/EPO_0000119</rdfs:seeAlso>
