    <!-- http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Skin -->

    <owl:Class rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Skin">
