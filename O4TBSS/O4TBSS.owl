    <!-- http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Urine -->

    <owl:Class rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Urine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Biological"/>
        <rdfs:subClassOf>
            <owl:Restriction>
