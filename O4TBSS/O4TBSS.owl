        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/EPO_0000133"/>
        <obo:IAO_0000115 xml:lang="en"> The ratio of the cumulative incidence rate in the exposed to the cumulative incidence rate in the unexposed. </obo:IAO_0000115>
        <obo:IAO_0000119>Dictionary of Epidemiology, Miquel Porta (Ed.)</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/epo.owl"/>
        <rdfs:label>cumulative incidence ratio</rdfs:label>
    </owl:Class>



    <!-- http://purl.obolibrary.org/obo/EPO_0000109 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/EPO_0000109">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/EPO_0000119"/>
        <obo:IAO_0000115 xml:lang="en"> See odds ratio.</obo:IAO_0000115>
        <obo:IAO_0000119>Dictionary of Epidemiology, Miquel Porta (Ed.)</obo:IAO_0000119>
