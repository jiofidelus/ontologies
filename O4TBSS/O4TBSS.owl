                    <rdf:Description rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Satisfactory_adh_Haynes_Sackett"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/parracarlos/ontologies/2019/3/untitled-ontology-31#Unsatisfactory_adh_Haynes_Sackett"/>
                </owl:unionOf>
